# Database connection string
DB_DSN=postgres://postgres:password@localhost:5432/flight_service_db?sslmode=disable

# Kafka brokers (comma-separated list)
KAFKA_BROKERS=localhost:9092

# Kafka topic name
KAFKA_TOPIC=flight_requests

# Kafka consumer group ID
KAFKA_GROUP_ID=flight_consumers

# HTTP server port
HTTP_PORT=8080