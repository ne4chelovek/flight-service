{
  "dashboard": {
    "id": null,
    "title": "Flight Service - Performance Metrics",
    "tags": ["flight-service", "performance"],
    "style": "dark",
    "timezone": "browser",
    "refresh": "5s",
    "schemaVersion": 27,
    "version": 1,
    "time": {
      "from": "now-15m",
      "to": "now"
    },
    "timepicker": {},
    "templating": {
      "list": []
    },
    "annotations": {
      "list": []
    },
    "editable": true,
    "gnetId": null,
    "graphTooltip": 0,
    "links": [],
    "panels": [
      {
        "id": 1,
        "title": "Channel Sizes",
        "type": "graph",
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 0,
          "y": 0
        },
        "targets": [
          {
            "expr": "channel_size",
            "legendFormat": "{{channel}}",
            "refId": "A"
          }
        ],
        "yAxes": [
          {
            "label": "Messages",
            "show": true
          }
        ]
      },
      {
        "id": 2,
        "title": "Go Routines",
        "type": "graph",
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 12,
          "y": 0
        },
        "targets": [
          {
            "expr": "go_goroutines",
            "legendFormat": "Goroutines",
            "refId": "A"
          }
        ],
        "yAxes": [
          {
            "label": "Count",
            "show": true
          }
        ]
      },
      {
        "id": 3,
        "title": "Memory Usage",
        "type": "graph",
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 0,
          "y": 8
        },
        "targets": [
          {
            "expr": "go_memstats_alloc_bytes",
            "legendFormat": "Allocated",
            "refId": "A"
          },
          {
            "expr": "go_memstats_sys_bytes",
            "legendFormat": "System",
            "refId": "B"
          }
        ],
        "yAxes": [
          {
            "label": "Bytes",
            "show": true
          }
        ]
      },
      {
        "id": 4,
        "title": "GC Duration",
        "type": "graph",
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 12,
          "y": 8
        },
        "targets": [
          {
            "expr": "histogram_quantile(0.95, rate(go_gc_duration_seconds_bucket[1m]))",
            "legendFormat": "GC Duration (p95)",
            "refId": "A"
          }
        ],
        "yAxes": [
          {
            "label": "Seconds",
            "show": true
          }
        ]
      }
    ],
    "refresh": "5s"
  }
}